{
  "manifest_version": 3,
  "name": "Disemb-Arc",
  "version": "2025.12.1704",
  "description": "A customizable new tab page with draggable bookmark boxes inspired by Arc Browser",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage",
    "bookmarks"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
